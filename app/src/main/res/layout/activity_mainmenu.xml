<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenuActivity">

    <Button
        android:id="@+id/한식"
        android:layout_width="162dp"
        android:layout_height="60dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_round_orange"
        android:fontFamily="@font/maruburiregular"
        android:text="#한식"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/반주"
        app:layout_constraintTop_toBottomOf="@+id/매운" />

    <Button
        android:id="@+id/분식"
        android:layout_width="118dp"
        android:layout_height="60dp"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_round_orange"
        android:fontFamily="@font/maruburiregular"
        android:text="#분식"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/해장"
        app:layout_constraintTop_toBottomOf="@+id/반주" />

    <Button
        android:id="@+id/매운"
        android:layout_width="211dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="145dp"
        android:background="@drawable/button_round_red"
        android:fontFamily="@font/maruburiregular"
        android:text="#매운"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/간식"
        app:layout_constraintTop_toBottomOf="@+id/what" />

    <Button
        android:id="@+id/안주"
        android:layout_width="94dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_round_green"
        android:fontFamily="@font/maruburiregular"
        android:text="#안주"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/분식"
        app:layout_constraintTop_toBottomOf="@+id/한식" />

    <Button
        android:id="@+id/간식"
        android:layout_width="137dp"
        android:layout_height="60dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="145dp"
        android:background="@drawable/button_round_pink"
        android:fontFamily="@font/maruburiregular"
        android:text="#간식"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/what" />

    <Button
        android:id="@+id/반주"
        android:layout_width="177dp"
        android:layout_height="60dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_round_green"
        android:fontFamily="@font/maruburiregular"
        android:text="#반주"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/간식" />

    <Button
        android:id="@+id/해장"
        android:layout_width="115dp"
        android:layout_height="60dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/button_round_red"
        android:fontFamily="@font/maruburiregular"
        android:text="#반주"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/반주" />

    <TextView
        android:id="@+id/what"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/maruburiregular"
        android:gravity="center"
        android:text="어떤 해시태그 음식이 먹고싶나요?"
        android:textColor="@android:color/black"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>